{
  "name": "mern-blog",
  "version": "1.0.0",
  "description": "Blog app",
  "main": "server.js",
  "scripts": {
    "build": "cd front-end && npm run build",
    "install-client": "cd front-end && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "start": "node back-end/server.js",
    "server": "nodemon back-end/server.js",
    "client": "npm run start --prefix front-end",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Mohammad Mo",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.30.0",
    "colors": "^1.4.0",
    "concurrently": "^7.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.5",
    "nodemon": "^2.0.16",
    "path": "^0.12.7"
  }
}
